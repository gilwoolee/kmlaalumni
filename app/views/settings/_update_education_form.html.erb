<%= form_for(@education, :url => education_path(@education), :html => { :method => :put }) do |f| %>

  <div><%= f.label :major %><br />
  <%= f.text_field :major %></div>
  
  <div><%= f.label :degree %><br />
  <%= f.text_field :degree %></div>
  
  <div><%= f.label :entrance_year %><br />
  <%= f.date_select :entrance_year, start_year: 1996, end_year: (Time.now.year + 5), discard_month: true, discard_day: true, include_blank: true, default: nil %></div>
  
  <div><%= f.label :graduation_year %><br />
  <%= f.date_select :graduation_year, start_year: (Time.now.year + 5), end_year: 1996, discard_month: true, discard_day: true, include_blank: true, default: nil %></div>

  <div><%= f.submit "저장" %></div>
  
<% end %>
