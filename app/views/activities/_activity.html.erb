<% if activity.feedable_type == 'Posting' %>
  <%= render partial: 'activities/posting', locals: {posting: activity.feedable, writeability: true, is_newsfeed: true} %>
<% elsif activity.feedable_type == 'Careernote' %>
  <%= render partial: 'activities/careernote', locals: {careernote: activity.feedable, view_type:'feed'} %>
<% end %>
